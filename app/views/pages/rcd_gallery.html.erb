<!DOCTYPE html>
<% meta title: "Royal Class Development Gallery | Royal Class Group"
%>
<body>
  <section class="rc-gallery">
    <div class="gallery-logo"><center>
      <%= link_to(rcd_path, html_options = {class: 'back-from-gallery-link'}) do  %> 
        <%= image_tag "https://s3.amazonaws.com/royalclass/arrows_slide_left1-min.png" %>
      <% end %>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
      <%= image_tag "https://s3.amazonaws.com/royalclass/rcd-color-min.png", :style => "width: 350px;", :alt => "Royal Class Development logo"%>
    </center></div>
    <div class="gallery-images">
      <div class="row">
        <% (1..7).each do |i| %>
          <div class="col-md-4">
            <div class="gallery-image-box" style="background: url(https://s3.amazonaws.com/royalclass/rcd-<%= i %>.jpg); -ms-background-size: cover; -o-background-size: cover; -moz-background-size: cover; -webkit-background-size: cover; background-size: cover; background-position: center center;"></div>
          </div>
        <% end %>
      </div>
    </div>
  </section>

</body>
